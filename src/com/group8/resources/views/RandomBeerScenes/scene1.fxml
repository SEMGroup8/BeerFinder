<Pane fx:id="pane" minHeight="400.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group8.controllers.RandomBeerControllers.Scene1Controller">
   <children>
      <Label layoutX="255.0" layoutY="50.0" text="Random beer generator .">
